services:
  mitiendams.api.autor:
    build:
      context: C:\Users\Miguel\source\repos\MiTiendaMS
      dockerfile: C:\Users\Miguel\source\repos\MiTiendaMS\MiTiendaMS.Api.Autor\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MiTiendaMS.Api.Autor
      target: base
    container_name: MiTiendaMS.Api.Autor
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: mitiendamsapiautor:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/MiTiendaMS.Api.Autor.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\Miguel\source\repos\MiTiendaMS\MiTiendaMS.Api.Autor
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Miguel\source\repos\MiTiendaMS
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Miguel\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Miguel\.nuget\packages\
